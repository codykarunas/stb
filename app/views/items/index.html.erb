<%= render "layouts/navbar" %>

<div class="global-wrapper">
  <div class="responsive-container">
    <div class="grid">
      <% @items.each do |item| %>
        <div class="cell">
          <%= link_to image_tag(item.image.url, alt: "Image"), item %><br />
          <%= item.name %>
        </div>
      <% end %>
    </div>
  </div>
</div>
